#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/@feathersjs+cli@5.0.12_@types+node@20.10.5/node_modules/@feathersjs/cli/bin/node_modules:/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/@feathersjs+cli@5.0.12_@types+node@20.10.5/node_modules/@feathersjs/cli/node_modules:/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/@feathersjs+cli@5.0.12_@types+node@20.10.5/node_modules/@feathersjs/node_modules:/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/@feathersjs+cli@5.0.12_@types+node@20.10.5/node_modules:/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/@feathersjs+cli@5.0.12_@types+node@20.10.5/node_modules/@feathersjs/cli/bin/node_modules:/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/@feathersjs+cli@5.0.12_@types+node@20.10.5/node_modules/@feathersjs/cli/node_modules:/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/@feathersjs+cli@5.0.12_@types+node@20.10.5/node_modules/@feathersjs/node_modules:/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/@feathersjs+cli@5.0.12_@types+node@20.10.5/node_modules:/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@feathersjs/cli/bin/feathers" "$@"
else
  exec node  "$basedir/../@feathersjs/cli/bin/feathers" "$@"
fi
