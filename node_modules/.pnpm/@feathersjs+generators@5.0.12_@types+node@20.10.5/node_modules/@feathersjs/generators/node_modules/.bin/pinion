#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/@feathershq+pinion@0.3.5_@types+node@20.10.5_typescript@5.3.3/node_modules/@feathershq/pinion/bin/node_modules:/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/@feathershq+pinion@0.3.5_@types+node@20.10.5_typescript@5.3.3/node_modules/@feathershq/pinion/node_modules:/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/@feathershq+pinion@0.3.5_@types+node@20.10.5_typescript@5.3.3/node_modules/@feathershq/node_modules:/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/@feathershq+pinion@0.3.5_@types+node@20.10.5_typescript@5.3.3/node_modules:/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/@feathershq+pinion@0.3.5_@types+node@20.10.5_typescript@5.3.3/node_modules/@feathershq/pinion/bin/node_modules:/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/@feathershq+pinion@0.3.5_@types+node@20.10.5_typescript@5.3.3/node_modules/@feathershq/pinion/node_modules:/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/@feathershq+pinion@0.3.5_@types+node@20.10.5_typescript@5.3.3/node_modules/@feathershq/node_modules:/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/@feathershq+pinion@0.3.5_@types+node@20.10.5_typescript@5.3.3/node_modules:/Users/prayag/prayag/apifier/apifier-core/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@feathershq+pinion@0.3.5_@types+node@20.10.5_typescript@5.3.3/node_modules/@feathershq/pinion/bin/pinion" "$@"
else
  exec node  "$basedir/../../../../../../@feathershq+pinion@0.3.5_@types+node@20.10.5_typescript@5.3.3/node_modules/@feathershq/pinion/bin/pinion" "$@"
fi
